<view><view class="image-container"><image class="main-image" src="{{goodsDetailData.image.url}}"></image></view><view class="info"><text class="title">{{goodsDetailData.title}}</text><view class="price-time"><u-row vue-id="f0bd6f06-1" justify="space-between" bind:__l="__l" vue-slots="{{['default']}}"><u-col vue-id="{{('f0bd6f06-2')+','+('f0bd6f06-1')}}" span="7" bind:__l="__l" vue-slots="{{['default']}}"><text class="favourPrice">{{goodsDetailData.favourPrice+"元"}}</text></u-col><u-col vue-id="{{('f0bd6f06-3')+','+('f0bd6f06-1')}}" span="5" offset="1" bind:__l="__l" vue-slots="{{['default']}}"><text class="time">{{goodsDetailData.serviceDuration+"分钟"}}</text></u-col><u-col vue-id="{{('f0bd6f06-4')+','+('f0bd6f06-1')}}" span="6" offset="2" bind:__l="__l" vue-slots="{{['default']}}"><text class="selection">{{goodsDetailData.salesVolume+"人选择"}}</text></u-col></u-row></view></view><view class="tabs"><text data-event-opts="{{[['tap',[['scrollTo',['introduction']]]]]}}" bindtap="__e">项目介绍</text><text data-event-opts="{{[['tap',[['scrollTo',['orderInfo']]]]]}}" bindtap="__e">下单须知</text></view><view data-ref="introduction" class="section vue-ref"><image class="project-introduction" src="{{goodsDetailData.imageList[0].url}}" mode="widthFix"></image></view><view data-ref="orderInfo" class="section vue-ref"><view class="order-info-container"><view class="order-instructions">下单须知:</view><text>{{goodsDetailData.OrderInstructions}}</text></view></view><button data-event-opts="{{[['tap',[['goto',['/pages/business/worker/worker?goodsID='+this.goodsID]]]]]}}" class="select-button" bindtap="__e">选择理疗师</button></view>