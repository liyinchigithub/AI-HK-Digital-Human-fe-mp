.content.data-v-0b0cdd8f {
	margin-left: 40rpx;
	margin-right: 40rpx;
}
.underline.data-v-0b0cdd8f {
	display: block;
	/* 确保元素是块级元素 */
	text-decoration: underline;
	font-weight: bold;
	margin-top: 30rpx;
	margin-bottom: 20rpx;
}
.table.data-v-0b0cdd8f {
	display: flex;
	flex-direction: column;
	width: 100%;
	border: 1px solid #ccc;
	/* 为整个表格设置边框 */
}
.table-header.data-v-0b0cdd8f, .table-row.data-v-0b0cdd8f {
  display: flex;
  flex-direction: row;
  border-bottom: 1px solid #ccc;
}
.table-cell.data-v-0b0cdd8f {
  flex: 1;
  padding: 10px;
  text-align: center;
  border-right: 1px solid #ccc;
  max-width: 150px;
  max-height: 800px;
  overflow: hidden;
  white-space: normal; /* 允许文本换行 */
  word-wrap: break-word; /* 强制长单词换行 */
  word-break: break-all; /* 强制长链接换行 */
}
.url.data-v-0b0cdd8f {
  color: #007aff;
  text-decoration: underline;
  word-wrap: break-word; /* 强制长链接换行 */
  word-break: break-all; /* 强制长链接换行 */
}
.table-cell.data-v-0b0cdd8f:last-child {
	border-right: none;
}

